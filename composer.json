{
  "name": "mehr-it/easy-xml",
  "description": "Easy XML handling (read/write) for large files",
  "minimum-stability": "dev",
  "license": "MIT",
  "authors": [
    {
      "name": "Christopher Mieger",
      "email": "mail@mehr-it.info"
    }
  ],
  "autoload": {
    "psr-4": {
      "MehrIt\\EasyXml\\": "src/"
    },
    "files": [
      "src/Build/build_functions.php"
